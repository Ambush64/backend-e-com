{
    "version": 2,
    "name": "backend-e-com",
    "builds": [{ "src": "./backend/server.js", "use": "@vercel/node" }],
    "routes": [
      { "handle": "filesystem" }, 
      { "src": "/api/(.*)", "dest": "./backend/server.js" }, 
      { "src": "/(.*)", "dest": "./backend/server.js" } 
    ]
  }
  